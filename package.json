{
  "name": "gas-html",
  "displayName": "gas-html",
  "description": "vs-code language support for Google App Script (GAS) html",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.61.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "ghtml",
        "aliases": [
          "gas-html",
          "ghtml"
        ],
        "extensions": [
          ".ghtml",
          ".gas-html"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "ghtml",
        "scopeName": "source.gas",
        "path": "./syntaxes/ghtml.tmLanguage.json"
      }
    ]
  }
}
