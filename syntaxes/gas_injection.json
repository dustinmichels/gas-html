{
  "scopeName": "gas-scriptlet.injection",
  "injectionSelector": "L:text.html.derivative",
  "patterns": [
    {
      "include": "#gas-scriptlet"
    }
  ],
  "repository": {
    "gas-scriptlet": {
      "begin": "(?=)(<\\?)",
      "beginCaptures": {
        "0": {
          "name": "gas.tag.begin"
        }
      },
      "end": "(?=)(\\?>)",
      "endCaptures": {
        "0": {
          "name": "gas.tag.end"
        }
      },
      "contentName": "meta.embedded.line.js",
      "patterns": [
        {
          "include": "source.js"
        }
      ]
    }
  }
}
